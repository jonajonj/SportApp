<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ParamActivity">


    <Button
        android:id="@+id/paramButtonSelectFile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/paramButtonDelDb"
        android:text="Select Config"/>


    <Button
        android:id="@+id/paramButtonDelDb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/paramButtonInitDb"
        android:text="Clear Base"/>

    <Button
        android:id="@+id/paramButtonInitDb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/paramButtonAddExercice"
        android:text="Init Base"/>

    <Button
        android:id="@+id/paramButtonAddExercice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="Ajout Exercice Manuel"/>



</androidx.constraintlayout.widget.ConstraintLayout>